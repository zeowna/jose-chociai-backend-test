{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/jose-chociai-backend-test/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/jose-chociai-backend-test/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/jose-chociai-backend-test",
  "projects": {
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "companies": {
      "type": "application",
      "root": "apps/companies",
      "entryFile": "main",
      "sourceRoot": "apps/companies/src",
      "compilerOptions": {
        "tsConfigPath": "apps/companies/tsconfig.app.json"
      }
    },
    "kafka": {
      "type": "library",
      "root": "libs/kafka",
      "entryFile": "index",
      "sourceRoot": "libs/kafka/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kafka/tsconfig.lib.json"
      }
    },
    "entities-definition": {
      "type": "library",
      "root": "libs/entities-definition",
      "entryFile": "index",
      "sourceRoot": "libs/entities-definition/src",
      "compilerOptions": {
